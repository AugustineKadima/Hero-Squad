{{#partial "content"}}

<h3>All Squads</h3>
<div class="card-wrapper" >
{{#each squad}}
<div class="card" style="width: 18rem;">
  <div class="card-body">
    <h4 class="card-title text text-primary">Squad name: <strong> {{ squadName }}</strong></h4>
    <hr>
    <p class="card-text">Cause: <strong>{{ cause }}</strong></p>

  </div>
</div>

{{/each}}
</div>


{{/partial}}

{{> layout.hbs}}